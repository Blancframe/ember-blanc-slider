{{#blanc-slider-list}}
    {{yield
        (hash
            slide=(component
                "slide" register=(action "registerItem") blancItems=blancItems
            )
            play=(action "play")
            stop=(action "stop")
            previous=(action "slidePrevious")
            next=(action "slideNext")
            autoPlayActive=autoPlayActive
            navigation=(component
                "slide-navigation"
                items=blancItems
                slideAction=(action "slideTo")
                activeBlancItem=activeBlancItem
            )
        )
    }}
{{/blanc-slider-list}}
