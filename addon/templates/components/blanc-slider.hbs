<div class="blanc-slider-container">
    <div class="blanc-slider-wrapper">
        {{
            yield
            (hash
                slide=(component
                    "slide"
                    register=(action "registerItem")
                    blancItems=blancItems
                )
            )
        }}
    </div>
</div>
<button onclick={{action "slidePrevious"}} type="button">
    Previous
</button>
<button onclick={{action "slideNext"}} type="button">
    Next
</button>
