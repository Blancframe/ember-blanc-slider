{{#each items as |item|}}
    {{#if hasBlock}}
        {{yield
            (hash
                index=item.index
                item=item.element.innerHTML
                slideAction=(action slideAction)
                isActive=item.isActive
                )
        }}
    {{else}}
        {{slide-navigation-item
            index=item.index
            slideAction=(action slideAction)
            isActive=item.isActive
        }}
    {{/if}}
{{/each}}
