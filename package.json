{
  "name": "ember-blanc-slider",
  "version": "1.0.0-beta-1.1",
  "description": "Accessible simple content slider",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blancframe/ember-blanc-slider.git"
  },
  "license": "MIT",
  "author": "jhonm <jhonmajoor@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "lint": "npm run lint:js && npm run lint:hbs",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "prettier": "prettier --write \"app/*/*.js\"",
    "pretty-quick": "pretty-quick"
  },
  "dependencies": {
    "ember-cli-babel": "^7.1.2",
    "ember-cli-htmlbars": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "broccoli-asset-rev": "^2.7.0",
    "coveralls": "^3.1.0",
    "ember-cli": "~3.7.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.7.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-template-lint": "^2.9.1",
    "ember-try": "^1.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "loader.js": "^4.7.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "qunit-dom": "^0.8.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && npm run lint && pretty-quick --staged && git add ."
    }
  },
  "bugs": {
    "url": "https://github.com/Blancframe/ember-blanc-slider/issues"
  },
  "homepage": "https://github.com/Blancframe/ember-blanc-slider#readme"
}
